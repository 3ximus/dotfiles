{
  "suggest.autoTrigger": "trigger",
  "suggest.insertMode": "insert",
  // "suggest.virtualText": true,
  "codeLens.enable": true,
  "codeLens.separator": "XX",
  "outline.autoPreview": true,
  "outline.togglePreviewKey": "p",
  "outline.keepWindow": false,
  "outline.checkBufferSwitch": false,
  //"diagnostic.virtualText": true,
  //"diagnostic.virtualTextCurrentLineOnly": false,
  //"diagnostic.virtualTextPrefix": " ",
  "diagnostic.errorSign": "#",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.currentFunctionSymbolDebounceTime": 50,
  "suggest.floatConfig": {
    "border": true,
    "highlight": "Normal",
    "borderhighlight": "GruvboxBg3"
  },
  "hover.floatConfig": {
    "border": true,
    "highlight": "Normal",
    "borderhighlight": "GruvboxBlueBold"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "highlight": "Normal",
    "borderhighlight": "GruvboxBg3"
  },
  "signature.floatConfig": {
    "border": true,
    "highlight": "Normal",
    "borderhighlight": "GruvboxBlueBold"
  },
  "dialog.floatBorderHighlight": "GruvboxAquaBold",
  "dialog.rounded": false,
  "dialog.floatHighlight": "GruvboxFg1",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/19.1.2/clangd_19.1.2/bin/clangd",
  // "typescript.referencesCodeLens.enabled": true,
  "python.analysis.typeCheckingMode": "off",
  "pyright.inlayHints.parameterTypes": false,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": false,
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--indent-size=2", "--ignore=E121"],
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.lens.enable": false,
  "languageserver": {
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    },
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  },
  "svelte.enable-ts-plugin": true,
  "snippets.ultisnips.pythonPrompt": false
}
