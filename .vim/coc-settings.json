{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "suggest.autoTrigger": "trigger",
  "suggest.insertMode": "insert",
  // "suggest.virtualText": true,
  "codeLens.enable": true,
  "codeLens.separator": "XX",
  "outline.autoPreview": true,
  "outline.togglePreviewKey": "p",
  "outline.keepWindow": false,
  "outline.checkBufferSwitch": false,
  "diagnostic.errorSign": ">",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "=",
  "diagnostic.hintSign": ">",
  "diagnostic.checkCurrentLine": true,
  // "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/15.0.6/clangd_15.0.6/bin/clangd",
  // "typescript.referencesCodeLens.enabled": true,
  "python.analysis.typeCheckingMode": "off",
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": false,
  "python.formatting.autopep8Args": ["--indent-size=2", "--ignore=E121"],
  "languageserver": {
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript"],
      "port": 6008
    },
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  },
  "svelte.enable-ts-plugin": true,
  "angular.enable-strict-mode-prompt": false
}
