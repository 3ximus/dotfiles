[user]
	email = fabio_r11@hotmail.com
	name = eximus
[push]
	default = simple
[core]
	editor = vim
[alias]
	unstage = reset HEAD --
	last = log -1 HEAD
	drop = checkout --
	logx = log --oneline --graph --decorate --pretty=format:'%C(yellow)%h%Creset %s (%Cblue%an%Creset, %cr)'
[diff]
	tool = vimdiff
[difftool]
	prompt = false
